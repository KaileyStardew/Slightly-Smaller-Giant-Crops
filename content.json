{
    "Format": "2.9.0",
	
	"ConfigSchema": {
		//Appearance Section
		"Cauliflower Sprite": {
			"AllowValues": "Default, 6480, BCAF",
            "Default": "Default",
			"Description": "If you select 6480 or BCAF and don't have the respective mod installed, the default sprite will be used.",
			"Section": "Appearance"
		},
		"Cauliflower Colour Scheme": {
			"AllowValues": "Vanilla, Desaturated",
            "Default": "Vanilla",
			"Description": "Make your cauliflower even paler. If your sprite selection is BCAF, this setting will do nothing.",
			"Section": "Appearance"
		},
		"Melon Sprite": {
			"AllowValues": "Default, Redrawn, Striped, 6480, BCAF, BCAF-Green",
            "Default": "Default",
			"Description": "If you select 6480 or BCAF and don't have the respective mod installed, the default sprite will be used.",
			"Section": "Appearance"
		},
		"Melon Colour Scheme": {
			"AllowValues": "Vanilla, Desaturated",
            "Default": "Vanilla",
			"Description": "A less vibrant pink. If your sprite selection is BCAF or BCAF Green, this setting will do nothing.",
			"Section": "Appearance"
		},
		"Pumpkin Colour Scheme": {
			"AllowValues": "Vanilla, Desaturated",
            "Default": "Vanilla",
			"Description": "Slightly less green pumpkin leaves. The pumpkin has no alternate sprite options.",
			"Section": "Appearance"
		},
		"Powdermelon Sprite": {
			"AllowValues": "Default, BCAF",
            "Default": "Default",
			"Description": "If you select BCAF and don't have BCAF installed, the default sprite will be used.",
			"Section": "Appearance"
		},
		"Powdermelon Colour Scheme": {
			"AllowValues": "Vanilla, Desaturated",
            "Default": "Vanilla",
			"Description": "A softer blue. If your sprite selection is BCAF, this setting will do nothing.",
			"Section": "Appearance"
		},
		"Qi Fruit Sprite": {
			"AllowValues": "Default, Glasses, Plain, BCAF",
            "Default": "Default",
			"Description": "If you select BCAF and don't have BCAF installed, the default sprite will be used.",
			"Section": "Appearance"
		},
		"Qi Fruit Colour Scheme": {
			"AllowValues": "Vanilla, Desaturated",
            "Default": "Vanilla",
			"Description": "A softer blue. If your sprite selection is BCAF, this setting will do nothing.",
			"Section": "Appearance"
		},
		//Behaviour Section
        "Cauliflower Chance":{
            "AllowValues":"low, normal, high, very high, false",
            "Default":"normal",
            "Section":"Behaviour"
        },
        "Melon Chance":{
            "AllowValues":"low, normal, high, very high, false",
            "Default":"normal",
            "Section":"Behaviour"
        },
        "Pumpkin Chance":{
            "AllowValues":"low, normal, high, very high, false",
            "Default":"normal",
            "Section":"Behaviour"
        },
        "Powdermelon Chance":{
            "AllowValues":"low, normal, high, very high, false",
            "Default":"normal",
            "Section":"Behaviour"
        },
        "Qi Fruit Chance":{
            "AllowValues":"low, normal, high, very high, false",
            "Default":"normal",
            "Section":"Behaviour"
        },
	},
